    "@react-navigation/native-stack": "^7.0.0",
    "expo-sqlite": "^15.0.0",
    "react": "18.3.1",
    "react-native": "0.76.0",
    "react-native-macos": "0.76.0",
    "react-native-document-picker": "^9.3.0",
