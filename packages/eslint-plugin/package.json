{
  "name": "@cmm/eslint-plugin",
  "version": "0.0.1",
  "description": "ESLint plugin to enforce design tokens in React Native styles",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint ."
