    "eslint": ">=9.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.0",
    "@types/estree": "^1.0.6",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "react-native",
    "design-tokens"
  ]
}
